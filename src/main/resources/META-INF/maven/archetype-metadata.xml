<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="play-scala-swagger-archetype"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <fileSets>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>conf</directory>
      <includes>
        <include>**/*.xml</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>app/controllers</directory>
      <includes>
        <include>**/*.scala</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>app/modules</directory>
      <includes>
        <include>**/*.scala</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>conf</directory>
      <includes>
        <include>**/*.</include>
        <include>**/*.conf</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>app/filters</directory>
      <includes>
        <include>**/*.scala</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>conf</directory>
      <includes>
        <include>routes</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>.gitignore</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>
